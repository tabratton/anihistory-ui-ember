    "@glimmer/tracking": "^1.1.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.16",
    "autoprefixer": "^10.4.21",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "concurrently": "^9.2.1",
