    "ember-concurrency": "^3.1.1",
    "ember-fetch": "^8.1.2",
    "ember-intl": "^6.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^8.2.0",
    "ember-power-select": "^7.1.2",
