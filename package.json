{
  "name": "anihistory-ui-ember",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for anihistory-ui-ember goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "pnpm run build:css && pnpm run build:js",
    "build:js": "ember build --environment=production",
    "build:css": "pnpx tailwindcss -i ./tailwind-input.css -o ./public/tailwind.css",
    "deploy": "pnpm run build:css && ember deploy production",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "start:css": "pnpx tailwindcss -i ./tailwind-input.css -o ./public/tailwind.css --watch",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
