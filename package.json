{
  "name": "anihistory-ui-ember",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for anihistory-ui-ember goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "yarn build:css && yarn build:js",
    "build:js": "ember build --environment=production",
    "build:css": "npx tailwindcss -i ./tailwind-input.css -o ./public/tailwind.css",
    "ci": "yarn install --frozen-lockfile",
    "deploy": "yarn build:css && ember deploy production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "start:css": "npx tailwindcss -i ./tailwind-input.css -o ./public/tailwind.css --watch",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/compat": "^2.1.1",
    "@embroider/core": "^2.1.1",
    "@embroider/webpack": "^2.1.1",
    "@fortawesome/ember-fontawesome": "^0.4.1",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@tailwindcss/forms": "^0.5.3",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.0.1",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~4.12.1",
    "ember-cli-app-version": "^6.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-aws-pack": "^3.0.0",
    "ember-cli-deploy-display-revisions": "^2.1.2",
    "ember-cli-deploy-revision-data": "^2.0.0",
    "ember-cli-deploy-s3-index": "^1.3.1",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-concurrency": "^3.0.0",
    "ember-data": "~4.11.3",
    "ember-fetch": "^8.1.2",
    "ember-intl": "^5.7.2",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^7.0.0",
    "ember-power-select": "^7.0.0",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^10.0.0",
    "ember-source": "~4.12.0",
    "ember-template-lint": "^5.7.2",
    "ember-truth-helpers": "^3.1.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.5.2",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.8",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "stylelint": "^15.6.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "tailwindcss": "^3.3.2",
    "tracked-built-ins": "^3.1.1",
    "webpack": "^5.81.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.35",
    "date-fns": "^2.29.3"
  }
}
